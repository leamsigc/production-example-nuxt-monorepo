{
    "version": "0.2.1",
    "configurations": [
        {
            "type": "node",
            "name": "Launch Nuxt Dev Server",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "pnpm",
            "runtimeArgs": [
                "dev"
            ],
            "console": "integratedTerminal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "env": {
                "NODE_ENV": "development"
            }
        },
