{
  "name": "bussines-api",
  "version": "0.0.1",
  "description": "Social media scheduler using Nuxt layers",
  "scripts": {
    "packages": "dotenv -- pnpm --filter",
    "install:all": "pnpm -r install",
    "site": "pnpm packages @local-monorepo/site build",
    "site:start": "pnpm packages @local-monorepo/site start",
    "site:dev": "pnpm packages @local-monorepo/site dev",
    "content": "pnpm packages @local-monorepo/content build",
    "content:start": "pnpm packages @local-monorepo/content start",
    "content:dev": "pnpm packages @local-monorepo/content dev",
    "connect:start": "pnpm packages @local-monorepo/connect start",
    "connect:dev": "pnpm packages @local-monorepo/connect dev",
    "connect": "pnpm packages @local-monorepo/connect build",
    "ai-tools": "pnpm packages @local-monorepo/ai-tools build",
    "ai-tools:start": "pnpm packages @local-monorepo/ai-tools start",
    "ai-tools:dev": "pnpm packages @local-monorepo/ai-tools dev",
    "tools": "pnpm packages @local-monorepo/tools build",
    "tools:start": "pnpm packages @local-monorepo/tools start",
    "tools:dev": "pnpm packages @local-monorepo/tools dev",
    "assets": "pnpm packages @local-monorepo/assets build",
    "assets:start": "pnpm packages @local-monorepo/assets start",
    "assets:dev": "pnpm packages @local-monorepo/assets dev",
    "auth": "pnpm packages @local-monorepo/auth build",
    "auth:start": "pnpm packages @local-monorepo/auth start",
    "auth:dev": "pnpm packages @local-monorepo/auth dev",
    "bulk-scheduler": "pnpm packages @local-monorepo/bulk-scheduler build",
    "bulk-scheduler:start": "pnpm packages @local-monorepo/bulk-scheduler start",
    "bulk-scheduler:dev": "pnpm packages @local-monorepo/bulk-scheduler dev",
    "email": "pnpm packages @local-monorepo/email build",
    "email:start": "pnpm packages @local-monorepo/email start",
    "email:dev": "pnpm packages @local-monorepo/email dev",
    "scheduler": "pnpm packages @local-monorepo/scheduler build",
    "scheduler:start": "pnpm packages @local-monorepo/scheduler start",
    "scheduler:dev": "pnpm packages @local-monorepo/scheduler dev",
    "ui": "pnpm packages @local-monorepo/ui build",
    "ui:start": "pnpm packages @local-monorepo/ui start",
    "ui:dev": "pnpm packages @local-monorepo/ui dev",
    "dev": "dotenv -- pnpm -r dev",
    "build": "dotenv -- pnpm -r build",
    "start": "pnpm --stream -r run dev",
    "clean": "pnpm -r run clean"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [],
  "author": "Ismael Garcia <leamsigc@leamsigc.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.19.0",
  "dependencies": {
    "dayjs": "^1.11.18"
  }
}
